using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class ArrayMy : MonoBehaviour
{
    // Start is called before the first frame update
    void Start()
    {
        int[] a = { -5 };
        Test(a);
        Debug.Log(a[0]);
    }
    static void Test(int[] x)
    {
        x[0] = 100;
    }

    // Update is called once per frame
    void Update()
    {
        
    }
}
